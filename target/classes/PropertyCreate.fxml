<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchor" prefHeight="1000.0" prefWidth="1400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PropertyCreateController">
   <children>
      <HBox alignment="CENTER" prefHeight="75.0" prefWidth="1405.0" spacing="130.0" style="-fx-background-color: #4A9FEA; -fx-border-color: #000000;">
         <children>
            <Text onMouseClicked="#sceneChange" strokeType="OUTSIDE" strokeWidth="0.0" text="Property Create">
               <font>
                  <Font size="35.0" />
               </font>
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
            </Text>
            <Text fx:id="mainmenuTV" onMouseClicked="#sceneChange" strokeType="OUTSIDE" strokeWidth="0.0" text="Main Menu">
               <font>
                  <Font size="35.0" />
               </font>
            </Text>
            <Text fx:id="logoutTV" onMouseClicked="#sceneChange" strokeType="OUTSIDE" strokeWidth="0.0" text="Logout">
               <font>
                  <Font size="35.0" />
               </font>
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </Text>
         </children>
      </HBox>
      <TextField fx:id="propertyNameField" layoutX="366.0" layoutY="90.0" prefHeight="40.0" prefWidth="280.0" promptText="&lt;Name&gt;" stylesheets="@text_field.css">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <TextField fx:id="propertyAddressField" layoutX="366.0" layoutY="177.0" prefHeight="40.0" prefWidth="280.0" promptText="&lt;Address&gt;" stylesheets="@text_field.css">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <TextArea fx:id="propertyDescArea" layoutX="366.0" layoutY="259.0" prefHeight="280.0" prefWidth="280.0" promptText="&lt;Description&gt;" stylesheets="@text_field.css">
         <font>
            <Font size="20.0" />
         </font></TextArea>
      <Spinner fx:id="numberOfRoomsSelector" layoutX="366.0" layoutY="593.0" prefHeight="40.0" prefWidth="280.0" stylesheets="@text_field.css" />
      <Text layoutX="93.0" layoutY="124.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Property Name:">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Text layoutX="60.0" layoutY="211.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Property Address:">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Text layoutX="6.0" layoutY="298.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Property Description:">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Text layoutX="41.0" layoutY="627.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Number of Rooms:">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <TextArea fx:id="roomTypeArea" layoutX="366.0" layoutY="690.0" prefHeight="280.0" prefWidth="280.0" promptText="Room Type,Count,Starting Room #" stylesheets="@text_field.css">
         <font>
            <Font size="15.0" />
         </font></TextArea>
      <Text layoutX="40.0" layoutY="731.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Room type, Count, Start Room #:" wrappingWidth="300.0">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <TextArea fx:id="amenitiesArea" layoutX="1111.0" layoutY="453.0" prefHeight="280.0" prefWidth="280.0" promptText="Amenity (one per line)" stylesheets="@text_field.css">
         <font>
            <Font size="20.0" />
         </font></TextArea>
      <Text layoutX="935.0" layoutY="492.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Amenities:">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <TextField fx:id="basePriceField" layoutX="1111.0" layoutY="767.0" prefHeight="40.0" prefWidth="280.0" promptText="0.00" stylesheets="@text_field.css">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <Text layoutX="784.0" layoutY="802.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Base Room Price: $">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <TextField fx:id="weekendRateField" layoutX="1111.0" layoutY="828.0" prefHeight="40.0" prefWidth="280.0" promptText="5.0" stylesheets="@text_field.css">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <Text layoutX="807.0" layoutY="862.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Weekend Rate: %">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
      <Button fx:id="addBtn" layoutX="769.0" layoutY="895.0" mnemonicParsing="false" onAction="#handleButtons" prefHeight="60.0" prefWidth="195.0" stylesheets="@button.css" text="Add Property">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="resetBtn" layoutX="1177.0" layoutY="896.0" mnemonicParsing="false" onAction="#handleButtons" prefHeight="60.0" prefWidth="195.0" stylesheets="@staffresetbtncss.css" text="Reset Fields">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="backBtn" layoutX="972.0" layoutY="895.0" mnemonicParsing="false" onAction="#handleButtons" prefHeight="60.0" prefWidth="195.0" stylesheets="@button.css" text="Back">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <TextArea fx:id="typeDescArea" layoutX="1116.0" layoutY="90.0" prefHeight="280.0" prefWidth="280.0" promptText="Room Type, Description (One per line)" stylesheets="@text_field.css">
         <font>
            <Font size="20.0" />
         </font>
      </TextArea>
      <Text layoutX="716.0" layoutY="129.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Room Type, Description:" wrappingWidth="400.0">
         <font>
            <Font size="36.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
